<Window x:Class="CryptoAES.DecryptionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CryptoAES"
        mc:Ignorable="d"
        Title="DecryptionWindow" Height="450" Width="800">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FF7BDF47"/>
            <GradientStop Color="White" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <PasswordBox HorizontalAlignment="Left" Margin="337,204,0,0" VerticalAlignment="Top" Width="192" Height="31" FontFamily="Arial" FontSize="16"/>
        <Label Content="Password&#xD;&#xA;" HorizontalAlignment="Left" Margin="241,202,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.513,0.002" Height="35" Width="95" FontFamily="Arial" FontSize="16"/>
        <Label Content="Decryption&#xD;&#xA;" HorizontalAlignment="Left" Margin="336,53,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.513,0.002" FontFamily="Arial" FontSize="24"/>
        <Label x:Name="opendownloadfolder" Content="Open Download Folder" HorizontalAlignment="Left" Margin="219,373,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.513,0.002" Height="34" Width="181" FontFamily="Arial" FontSize="16"/>
        <Image HorizontalAlignment="Left" Height="100" Margin="98,0,0,0" VerticalAlignment="Center" Width="100"/>
        <TextBox HorizontalAlignment="Left" Margin="241,118,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsReadOnly="True"/>
        <Button x:Name="browes" Content="Button" HorizontalAlignment="Left" Margin="433,127,0,0" VerticalAlignment="Top" Click="browes_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="361,302,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
